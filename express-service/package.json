{
  "name": "express-service",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=dev nodemon src/index/js",
    "start-win": "SET NODE_ENV=dev&&nodemon src/index.js",
    "test": "NODE_ENV=test mocha --timeout 10000 --exit --recursive",
    "test-win": "SET NODE_ENV=test&&mocha --timeout 10000 --exit --recursive",
    "coverage": "NODE_ENV=test c8 -r html npm test",
    "coverage-win": "SET NODE_ENV=test&& c8 -r text npm run test-win",
    "start-prod": "NODE_ENV=production node src/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "mongodb": "^4.0.0"
  },
  "devDependencies": {
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.3",
    "sinon": "^18.0.0",
    "supertest": "^7.0.0"
  }
}
